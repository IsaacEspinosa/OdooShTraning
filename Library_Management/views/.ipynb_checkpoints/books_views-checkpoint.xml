        <record model="ir.ui.view" id="book_view_form">
            <field name="name">book.view.form</field>
            <field name="model">library.book</field>
            <field name="arch" type="xml">
                <form string="Formulario de tareas">
                    <sheet>
                        <h1><field name="name"/></h1>
                        <group>
                            <group>
                                <field name="author"/>
                                <field name="editors"/>
                            </group>
                            <group>
                                <field name="ISBN"/>
                                <field name="gender"/>
                                <field name="publisher"/>
                                <field name="editionYear"/>
                            </group>
                        </group>
                            <notebook>
                                <page string="Texto de nota">
                                    <field name="noteText"/>
                                </page>
                            </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="book_view_search">
            <field name="name">book.view.search</field>
            <field name="model">library.book</field>
            <field name="arch" type="xml">
                <search>
                    <field name="author"/>
                    <field name="ISBN"/>
                </search>
            </field> 
        </record>
    </data>      
</odoo>